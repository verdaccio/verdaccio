    "cors": "2.8.5",
    "date-fns": "1.30.1",
    "express": "4.16.4",
    "global": "4.3.2",
    "handlebars": "4.0.12",
    "http-errors": "1.7.1",
    "js-base64": "2.5.1",
    "js-string-escape": "1.0.1",
    "js-yaml": "3.12.1",
    "jsonwebtoken": "8.4.0",
    "lockfile": "1.0.4",
    "lodash": "4.17.11",
    "lunr-mutable-indexes": "2.3.2",
    "marked": "0.6.0",
    "mime": "2.4.0",
    "minimatch": "3.0.4",
    "mkdirp": "0.5.1",
    "mv": "2.1.1",
    "pkginfo": "0.4.1",
    "request": "2.88.0",
