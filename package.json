{
  "name": "verdaccio",
  "version": "4.0.0-alpha.4",
  "description": "Private npm repository server",
  "author": {
    "name": "Verdaccio Maintainers",
    "email": "verdaccio.npm@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/verdaccio/verdaccio"
  },
  "main": "build/index.js",
  "bin": {
    "verdaccio": "./bin/verdaccio"
  },
  "dependencies": {
