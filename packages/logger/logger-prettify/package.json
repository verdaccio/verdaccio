{
  "name": "@verdaccio/logger-prettify",
  "version": "8.0.0-next-8.4",
  "description": "Verdaccio Logger Prettify",
  "main": "./build/index.js",
  "types": "build/index.d.ts",
  "author": {
    "name": "Juan Picado",
    "email": "juanpicado19@gmail.com"
  },
  "repository": {
    "type": "https",
    "url": "https://github.com/verdaccio/verdaccio",
