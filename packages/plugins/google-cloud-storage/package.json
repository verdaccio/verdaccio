{
  "name": "verdaccio-google-cloud",
  "version": "11.0.0-6-next.9",
  "description": "Google Cloud storage implementation for Verdaccio",
  "keywords": [
    "private",
    "package",
    "repository",
    "registry",
    "enterprise",
    "modules",
    "proxy",
    "server",
    "verdaccio"
  ],
  "author": "Juan Picado <juanpicado19@gmail.com>",
  "license": "MIT",
  "homepage": "https://verdaccio.org",
  "repository": {
    "type": "https",
    "url": "https://github.com/verdaccio/verdaccio",
    "directory": "packages/plugins/google-cloud-storage"
  },
  "bugs": {
    "url": "https://github.com/verdaccio/verdaccio/issues"
  },
  "exports": "./build/index.js",
  "type": "module",
  "types": "build/index.d.ts",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6"
  },
  "dependencies": {
    "@google-cloud/datastore": "6.6.2",
    "@google-cloud/storage": "5.18.2",
    "@verdaccio/core": "workspace:6.0.0-6-next.4",
    "@verdaccio/streams": "workspace:11.0.0-6-next.5"
  },
  "devDependencies": {
    "@verdaccio/types": "workspace:11.0.0-6-next.10",
    "memory-fs": "0.5.0"
  },
  "optionalDependencies": {
    "fast-crc32c": "1.0.7"
  },
  "scripts": {
    "clean": "rimraf ./build",
    "type-check": "tsc --noEmit -p tsconfig.build.json",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "build:js": "babel src/ --out-dir build/ --copy-files --extensions \".ts,.tsx\" --source-maps",
    "build": "pnpm run build:js && pnpm run build:types",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/verdaccio"
  }
}
