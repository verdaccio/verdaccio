    "@verdaccio/logger": "workspace:6.0.0-6-next.12",
    "@verdaccio/utils": "workspace:6.0.0-6-next.12",
    "JSONStream": "1.3.5",
    "debug": "4.3.4",
    "lodash": "4.17.21",
    "got": "11.8.5",
    "hpagent": "1.0.0",
    "undici": "4.16.0"
  },
  "devDependencies": {
    "@types/node": "16.11.51",
